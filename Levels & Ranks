#include <iostream>
using namespace std;

void checkRank(int level)
{
  if(level <= 5)
  cout << "Your Rank is Bronze";
  else if(level <= 10)
  cout << "Your Rank is Silver";
  else if(level <= 15)
  cout << "Your Rank is Gold";
  else if(level <= 20)
  cout << "Your Rank is Platinum";
  else if(level <= 25)
  cout << "Your Rank is Diamond";


}

int main()
{
  int yourLevel;
  cout << "Enter your CodeDex Level: ";
  cin >> yourLevel;
  checkRank(yourLevel);
  
}
