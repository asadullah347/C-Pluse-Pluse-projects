#include <iostream>
using namespace std;

double MonthOverMonth(double m1, double m2)
{
  double mom = ((m2 - m1) / m1) *  100;
  return mom;
}

int main() {
  double m1,m2;
  cout << "Enter Month1:\n";
  cin >> m1;
  cout << "Enter Month2:\n";
  cin >> m2;


  double mom = MonthOverMonth(m1,m2);
  cout << "Month-over-Month change: " << mom << "%" << endl;

    return 0;
}
